<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.it.yx.dao.StudentDao">
    <select id="selectAllStudent" resultMap="getAllStudentMap">
        select * from springbootdb.student;
    </select>
    <resultMap id="getAllStudentMap" type="com.it.yx.pojo.Student">
        <id property="id" column="id"/>
        <result property="stu_name" column="stu_name"/>
        <result property="stu_age" column="stu_age"/>
        <result property="stu_addr" column="stu_addr"/>
    </resultMap>
</mapper>

